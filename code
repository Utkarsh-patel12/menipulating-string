j=-1
v=[]
n=[]
q=''
s=['a','b','c','d','e','f','g','h','i','j','k','l','m','n','o','p','q','r','s','t','u','v','w','x','y','z']
a=input('Enter a number')
l=' '
h=a+l
b=len(a+l)

for i in range(0,b):
    c=h[i]
    n.append(c)
    if c in s:
        v.append(c)
        


for r in list(v):
    n.remove(r)
   

w=len(n)
for r in range(0,w):
    f=n[r]
    q=q+f

e=len(q)
for i in range(0,e):
    g=q[i]
    if g==' ':
            d=q[j+1:i]
            j=i
            print(d)
